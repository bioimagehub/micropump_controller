    def reset_attachment_state(self) -> bool:
        """Reset USB attachment state to force re-attachment on next initialization.
        
        Useful for troubleshooting or when the USB device has been changed.
        
        Returns:
            True if state was reset successfully, False otherwise.
        """
        try:
            state_file = self._get_attachment_state_file()
